{
  "name": "droopy-dropdown",
  "version": "1.0.2",
  "description": "A better, more accessible, dropdown menu",
  "main": "dist/index.js",
  "targets": {
    "default": {}
  },
  "scripts": {
    "start": "parcel demo/index.html --target default",
    "test": "jest --coverage",
    "prettier": "prettier --write .",
    "lint": "npx eslint src --ext .ts --fix",
    "build": "npx rollup -c",
    "build:docs": "npx rimraf docs/* && npx parcel build demo/index.html --dist-dir docs --public-url . --target default"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevoneil/droopy.git"
  },
  "keywords": [
    "a11y",
    "dropdown",
    "droopy",
    "autocomplete",
    "search bar",
    "search"
  ],
  "author": {
    "name": "Kevin O'Neil (@kevoneil)",
    "email": "kevinmoneil@live.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevoneil/droopy/issues"
  },
  "homepage": "https://github.com/kevoneil/droopy#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/jest": "^26.0.22",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.5.0",
    "prettier": "2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.32.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
